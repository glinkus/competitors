{
    "targets": "defaults",
    "presets": [
        [
            "@babel/preset-env",
            {
                "corejs": 3,
                "debug": false,
                "useBuiltIns": "usage"
            }
        ],
        [
            "@babel/preset-typescript",
            {
                "allowDeclareFields": true
            }
        ]
    ],
    "plugins": [
        "babel-plugin-syntax-hermes-parser",
        [
            "@babel/plugin-proposal-decorators",
            {
                "version": "2023-11"
            }
        ]
    ]
}